      <single>
        <search>
          <query>| mstats max("f0") prestats=true WHERE index="s4c_phyphox_metrics" span=1s 
| timechart max("f0") AS max_f0 span=1s 
| eval max_f0 = round(max_f0, 1)
| lookup notes_frequency_0_4000_tenth.csv max_f0</query>
